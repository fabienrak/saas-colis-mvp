                                  <tbody>
                                    <tr>
                                      <th scope="row">Register Packages</th>
                                      <td>WIL000010</td>
                                      <td><button class="btn btn-primary btn-square px-4 b-r-10 track_data" data-type="1" data-invoice="WIL000010">Track</button></td>
                                    </tr>
                                    <tr>
                                      <th scope="row">Shipment</th>
                                      <td>AWB000015</td>
                                      <td><button class="btn btn-primary btn-square px-4 b-r-10 track_data" data-type="2" data-invoice="AWB000015">Track</button></td>
                                    </tr>
                                    <tr>
                                      <th scope="row">Pickup</th>
                                      <td>ABC00003</td>
                                      <td><button class="btn btn-primary btn-square px-4 b-r-10 track_data" data-type="3" data-invoice="ABC00003">Track</button></td>
                                    </tr>
                                    <tr>
